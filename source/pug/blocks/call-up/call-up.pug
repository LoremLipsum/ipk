mixin call-up(data)
    +el({
        class: 'call-up',
        mix: data.mix,
        mod: data.mod,
        js: data.js,
    })&attributes(attributes)
        +el({
            tag: 'span',
            class: 'call-up__text',
            text: 'Консультация специалиста'
        })
        +el({
            tag: 'a',
            href: 'tel:+74954545555',
            class: 'call-up__link',
            text: '+7(495) 454 5555',
        })
