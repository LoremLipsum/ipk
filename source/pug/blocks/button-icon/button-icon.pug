mixin button-icon(data)
    +el({
        tag: data.tag ? data.tag : 'button',
        class: 'button-icon',
        mix: data.mix,
        mod: data.mod,
        js: data.js,
        before: data.before,
        after: data.after,
        position: data.position,
        title: data.title,
        target: data.target,
        href: data.href,
        ariaLabel: data.ariaLabel
    })&attributes(attributes)
        +el({
            tag: 'img',
            image: data.icon,
            class: 'button-icon__icon'
        })
